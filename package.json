{
  "name": "@nimpl/inio",
  "version": "0.9.0",
  "description": "Utility for editing a group of JSON files",
  "bin": {
    "inio": "./dist/index.js",
    "nimpl-inio": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "nimpl:inio": "pnpm --filter @nimpl/inio",
    "lint": "eslint .",
    "eslint": "eslint",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "i18n",
    "inio",
    "json",
    "editor",
    "nimpl"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/vordgi/nimpl-inio.git"
  },
  "author": {
    "name": "Savelyev Alexander",
    "email": "vordgi1@gmail.com",
    "url": "https://github.com/vordgi/"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "20.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "prettier": "3.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "typescript-eslint": "7.13.0"
  },
  "dependencies": {
    "glob": "10.4.2",
    "socket.io": "4.7.5"
  }
}